{
  "smarts_atom_primitives": [
    { "symbol": "*", "name": "wildcard", "requirement": "any atom", "default": null },
    { "symbol": "a", "name": "aromatic", "requirement": "aromatic", "default": null },
    { "symbol": "A", "name": "aliphatic", "requirement": "aliphatic", "default": null },
    { "symbol": "D<n>", "name": "degree", "requirement": "<n> explicit connections", "default": "exactly one" },
    { "symbol": "H<n>", "name": "total-H-count", "requirement": "<n> attached hydrogens", "default": "exactly one" },
    { "symbol": "h<n>", "name": "implicit-H-count", "requirement": "<n> implicit hydrogens", "default": "at least one" },
    { "symbol": "R<n>", "name": "ring membership", "requirement": "in <n> SSSR rings", "default": "any ring atom" },
    { "symbol": "r<n>", "name": "ring size", "requirement": "in smallest SSSR ring of size <n>", "default": "any ring atom" },
    { "symbol": "v<n>", "name": "valence", "requirement": "total bond order <n>", "default": "exactly one" },
    { "symbol": "X<n>", "name": "connectivity", "requirement": "<n> total connections", "default": "exactly one" },
    { "symbol": "x<n>", "name": "ring connectivity", "requirement": "<n> total ring connections", "default": "at least one" },
    { "symbol": "-<n>", "name": "negative charge", "requirement": "-<n> charge", "default": "-1 charge" },
    { "symbol": "+<n>", "name": "positive charge", "requirement": "+<n> formal charge", "default": "+1 charge" },
    { "symbol": "#n", "name": "atomic number", "requirement": "atomic number <n>", "default": null },
    { "symbol": "@", "name": "chirality", "requirement": "anticlockwise", "default": "anticlockwise (default class)" },
    { "symbol": "@@", "name": "chirality", "requirement": "clockwise", "default": "clockwise (default class)" },
    { "symbol": "@<c><n>", "name": "chirality", "requirement": "chiral class <c> chirality <n>", "default": null },
    { "symbol": "@<c><n>?", "name": "chiral or unspec", "requirement": "chirality <c><n> or unspecified", "default": null },
    { "symbol": "<n>", "name": "atomic mass", "requirement": "explicit atomic mass", "default": "unspecified mass" }
  ],

